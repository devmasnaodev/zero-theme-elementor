{
	"name": "infografic/zero-theme",
	"description": "A simple and lightweight theme for Elementor page builder based on Tailwindcss or SASS",
	"type": "project",
	"require-dev": {
		"giacocorsiglia/wordpress-stubs": "^5.1",
		"wp-coding-standards/wpcs": "^3.0",
		"phpcompatibility/phpcompatibility-wp": "*"
	},
	"license": "GPL-2.0-or-later",
	"authors": [
		{
			"name": "Rodrigo Gomes",
			"email": "rodrigo@infografic.com.br"
		}
	],
	"autoload": {
		"psr-4": {
			"ZT\\Core\\": "core/",
			"ZT\\Includes\\": "includes/"
		}
	},
	"minimum-stability": "stable",
	"config": {
		"allow-plugins": {
			"dealerdirect/phpcodesniffer-composer-installer": true
		}
	}
}
